for v in dic.values(): #v[1]がテンプレートの値
    for i in range(len(v[1])):
        if v[1][i] == SQM:
            q_count = q_count + 1
            if q_count >= 2:
                q_flag = 1
        else:
            if q_flag == 1:
                q_keep = q_count
                q_flag = 0
                q_maxV = i
                for j in range(q_keep):
                    #v[1][i-j-1].replace(SQM, '')
                    pattern.sub('', v[1][i-j-1])
                    print (v[1][i-j-1])
                #print ('←ここ境界線')
            q_count = 0
        sys.stdout.write(v[1][i])
    print ('')
